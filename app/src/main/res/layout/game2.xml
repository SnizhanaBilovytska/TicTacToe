<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clickable="false"
    android:background="@drawable/polotno">


    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button22"
        android:height="110dp"
        android:visibility="visible"
        android:width="80dp"
        android:background="#00ffffff"
        android:layout_alignTop="@+id/button21"
        android:layout_centerHorizontal="true"
        android:clickable="false" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button31"
        android:height="140dp"
        android:visibility="visible"
        android:layout_below="@+id/button22"
        android:layout_toStartOf="@+id/button22"
        android:width="100dp"
        android:layout_marginTop="15dp"
        android:background="#00ffffff"
        android:layout_toLeftOf="@+id/button22"
        android:layout_marginRight="10dp" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button21"
        android:height="110dp"
        android:width="100dp"
        android:background="#00ffffff"
        android:layout_centerVertical="true"
        android:layout_alignLeft="@+id/button31"
        android:layout_alignStart="@+id/button31"
        android:visibility="visible" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button11"
        android:height="120dp"
        android:visibility="visible"
        android:width="90dp"
        android:layout_alignBottom="@+id/button13"
        android:singleLine="false"
        android:layout_alignRight="@+id/button21"
        android:layout_alignEnd="@+id/button21"
        android:clickable="false"
        android:background="#00ffffff"
        android:layout_alignLeft="@+id/button21"
        android:layout_alignStart="@+id/button21" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button13"
        android:height="125dp"
        android:visibility="visible"
        android:layout_above="@+id/button21"
        android:layout_toRightOf="@+id/button22"
        android:layout_toEndOf="@+id/button22"
        android:width="100dp"
        android:background="#00ffffff"
        android:layout_marginLeft="15dp"
        android:layout_marginBottom="20dp" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button23"
        android:height="130dp"
        android:visibility="visible"
        android:layout_toEndOf="@+id/button22"
        android:width="100dp"
        android:background="#00ffffff"
        android:layout_above="@+id/button31"
        android:layout_alignLeft="@+id/button13"
        android:layout_alignStart="@+id/button13"
        android:layout_alignRight="@+id/button13"
        android:layout_alignEnd="@+id/button13"
        android:layout_alignTop="@+id/button22" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button32"
        android:height="130dp"
        android:visibility="visible"
        android:width="80dp"
        android:layout_centerHorizontal="true"
        android:layout_alignTop="@+id/button33"
        android:background="#00ffffff" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button12"
        android:layout_toEndOf="@+id/button11"
        android:height="125dp"
        android:width="90dp"
        android:visibility="visible"
        android:layout_alignTop="@+id/button11"
        android:layout_alignBottom="@+id/button11"
        android:layout_alignRight="@+id/button32"
        android:layout_alignEnd="@+id/button32"
        android:background="#00ffffff"
        android:layout_alignLeft="@+id/button32"
        android:layout_alignStart="@+id/button32" />

    <Button
        style="?android:attr/buttonStyleSmall"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/button33"
        android:width="80dp"
        android:height="140dp"
        android:visibility="visible"
        android:layout_toEndOf="@+id/button22"
        android:clickable="false"
        android:background="#00ffffff"
        android:focusableInTouchMode="true"
        android:layout_alignTop="@+id/button31"
        android:layout_alignLeft="@+id/button23"
        android:layout_alignStart="@+id/button23"
        android:layout_alignRight="@+id/button23"
        android:layout_alignEnd="@+id/button23" />

</RelativeLayout>